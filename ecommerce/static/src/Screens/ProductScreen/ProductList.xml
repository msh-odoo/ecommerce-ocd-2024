<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="ecommerce.ProductList">
        <div class="col-9">
            <div class="row">
                <t t-foreach="props.products" t-as="product" t-key="product.id">
                    <ProductItem product="product"/>
                </t>
            </div>
        </div>
    </t>
</templates>
